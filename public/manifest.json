{
  "name": "Tag Window",
  "description": "gain a better overview of your browser windows (eg. inside Application Expos√©) by tagging them with emojis",
  "version": "1.0",
  "manifest_version": 3,
  "commands": {
    "_execute_action": {
      "suggested_key": "Alt+Shift+T",
      "description": "Open the popup"
    },
    "hello": {
      "suggested_key": "Ctrl+Shift+U",
      "description": "Say Hello"
    }
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["./static/js/content.js"]
    }
  ],
  "permissions": ["tabs", "scripting", "storage"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}
